{
  "name": "note-web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier -c \"**/*.+(js|json|ts|tsx)\"",
    "format:fix": "prettier -c --write \"**/*.+(js|json|ts|tsx)\"",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "validate": "run-p check-types && lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^5",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "4.5.5"
  }
}
